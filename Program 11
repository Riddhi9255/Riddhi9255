#include <iostream>
using namespace std;

int main() {
    char ch;
     cout<<"riddhi memdavadi";
      cout<<"220130318035";
    cout << "Enter a character: ";
    cin >> ch;

    if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u' ||
        ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U') {
        cout << "It's a vowel." << endl;
    } else {
        cout << "It's a consonant." << endl;
    }

    return 0;
}
